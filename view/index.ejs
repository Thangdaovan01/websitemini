    <div class="post-container">
        <div class="post-header-container">
            <div class="post-header-container1">
                <div class="image-container user-page" data-value=" ">
                    <img src="" alt="Your Image">
                </div>
                <div class="input-container">
                    <input type="text" placeholder="Enter your content" >
                    <!-- Placeholder for additional content -->
                </div>
            </div>
            
            <div class="icons-container">
                <button id="createpostimage" title="Thêm ảnh" class="btn create-post-image-btn btn-link"><i class="fa-regular fa-image"></i> Thêm ảnh</button>
                <button id="createpostvideo" title="Thêm video" class="btn create-post-video-btn btn-link"><i class="fa-solid fa-video"></i> Thêm video</button>
                <button id="createfile" title="Thêm file" class="btn create-file-btn btn-link"><i class="fa-regular fa-file"></i> Tài liệu</button>
            </div>
        </div>

                
        <div class = post-content-container>

        </div>
    </div>
    <div class="window"></div>   
    <div class="description"></div>
    <div id="notification" class="notification">
        <span id="notificationText"></span>
    </div>    
